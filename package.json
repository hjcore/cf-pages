{
  "name": "cf-pages",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:cf": "wrangler pages dev out --local --port 3000 --compatibility-date=2023-01-07",
    "build": "next lint && next build",
    "export": "next export && cp -a _routes.json out/",
    "start": "next start",
    "clean": "rm -rf out node_modules .next",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.10.0",
    "@mantine/hooks": "^5.10.0",
    "@mantine/next": "^5.10.0",
    "@mantine/notifications": "^5.10.0",
    "@next/eslint-plugin-next": "^13.1.2",
    "@tabler/icons": "^1.119.0",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.8.3",
    "typescript": "4.9.4",
    "wrangler": "^2.7.1"
  }
}
